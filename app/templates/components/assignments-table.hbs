<h1>Assignments</h1>

<ul>
  {{#each assignmentService.assignments as |assignment|}}
  <li>
    {{assignment.title}}, {{assignment.instructions}}

    {{button-with-confirmation warning="Are you sure you want to delete this assignment?" text='Delete'
    onConfirm=(action "deleteAssignment" assignment.id)}}
  </li>
  {{/each}}
</ul>